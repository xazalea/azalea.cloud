# Application URL
NEXT_PUBLIC_APP_URL=http://localhost:3000

# WebSocket URL (for VNC connections)
NEXT_PUBLIC_WS_URL=ws://localhost:3000/api/vnc/ws

# VNC Server Configuration
# These are the settings for your Windows 10 VM with VNC server
VNC_SERVER_HOST=localhost
VNC_SERVER_PORT=5900
VNC_PASSWORD=

# Optional: VNC WebSocket Proxy
VNC_WS_PROXY_URL=

# Node Environment
NODE_ENV=development

