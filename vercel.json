{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/cloudshell/:path*",
      "destination": "/api/cloudshell/:path*"
    },
    {
      "source": "/clienterror/:path*",
      "destination": "/api/clienterror/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
